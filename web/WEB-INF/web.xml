<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>Dashboard_LecturerServlet</servlet-name>
        <servlet-class>com.fyp.controller.login.Dashboard_LecturerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.fyp.controller.login.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddLecturerDao</servlet-name>
        <servlet-class>com.fyp.model.Dao.login.AddLecturerDao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddLecturerDAO</servlet-name>
        <servlet-class>com.fyp.lecturer.dao.AddLecturerDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddLecturerServlet</servlet-name>
        <servlet-class>com.fyp.controller.lecturer.AddLecturerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Dashboard_LecturerServlet</servlet-name>
        <url-pattern>/Dashboard_LecturerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddLecturerDao</servlet-name>
        <url-pattern>/AddLecturerDao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddLecturerDAO</servlet-name>
        <url-pattern>/AddLecturerDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddLecturerServlet</servlet-name>
        <url-pattern>/AddLecturer</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>LoginID.jsp</welcome-file>
    </welcome-file-list>
</web-app>
